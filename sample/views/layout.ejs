	<script>
		$(document).ready(function () {
			var start = function () {
				$("#ajaxStatus").fadeIn("slow")
			}

			var stop = function () {
				$("#ajaxStatus").fadeOut("slow")
			}

			JQU.ajax.addStartEventHandler(start)
			JQU.ajax.addStopEventHandler(stop)
		})
	</script>

